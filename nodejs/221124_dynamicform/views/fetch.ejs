<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js">
    </script>
    <script>
        function sendGet() { 
            fetch("/fetch?name=이름", {
            method: "get",
            }).then((response) => console.log(response.json()));
        }

         function sendPost() {
            fetch("/fetch", {
            method: "post",
            headers: {
            "Content-Type": "application/json",
        },
            body: JSON.stringify({
		    name: '이름'
        }),
            }).then((response) => console.log(response.json()));
        }
    </script> 
</head>
<body>
    <form id="form_info">
        <fieldset>
            <legend>fetch</legend>
            이름 <input type="text" name="name" required placeholder="ID"><br>
            성별 <input type="radio" name="gender" value="남성" id="남성"><label for="남성" required>남성</label>
                 <input type="radio" name="gender" value="여성" id="여성" required><label for="여성">여성</label><br>
           생년월일<input type="date" name="date">
        </fieldset>
        <button type="button" onclick="sendGet();">get가입</button>
        <button type="button" onclick="sendPost();">post가입</button>
        </form>    
</body>
</html>
